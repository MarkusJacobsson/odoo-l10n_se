<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <act_window id="action_wizard_reconcile_model_journal" name="Set Journal" res_model="wizard.reconcile.model.journal" src_model="account.reconcile.model" view_mode="form" target="new" multi="True" />
    <record id="wizard_reconcile_model_journal_form" model="ir.ui.view">
        <field name="name">Set Journal Form</field>
        <field name="model">wizard.reconcile.model.journal</field>
        <field name="arch" type="xml">
            <form string="Set Journal">
                <group>
                    <field name="journal_id" />
                </group>
                <footer>
                    <button string="Set Journal" name="set_journal" type="object" class="or_right oe_highlight" />
                    or
                    <button string="Cancel" class="oe_right" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
